<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <DockerDefaultTargetOS>Windows</DockerDefaultTargetOS>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="CSCore" Version="1.2.1.2" />
        <PackageReference Include="Magick.NET-Q16-AnyCPU" Version="11.3.0" />
    </ItemGroup>

    <ItemGroup>
        <None Remove="Assets\TestComparison\1.jpg" />
        <Content Include="Assets\TestComparison\1.jpg">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </Content>
        <None Remove="Assets\TestComparison\2.jpg" />
        <Content Include="Assets\TestComparison\2.jpg">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </Content>
    </ItemGroup>
</Project>
